<?php

/**
 * Implements hook_menu()
 */
function ns_site_menu() {
	$items = array();
	
	$items['projects'] = array (
		'type' => MENU_CALLBACK,
		'title' => t('Projects'),
		'page callback' => 'ns_site_projects',
		'access arguments' => array('access content'),
		'file' => 'ns_site.projects.inc',
	);

	return $items;
}